<form id="form_inreg" method="post" class="date" action="<%= locals.action %>" enctype="multipart/form-data">

    <p><label>Username: <input type="text" name="username" id="inp-username"
                value="guest<%= Math.floor(Math.random()*100000) %>" required pattern="[A-Za-z0-9#_./]+"></label></p>
    <p><label>Nume: <input type="text" name="nume" id="inp-nume" value="Last name"></label></p>
    <p><label>Prenume: <input type="text" name="prenume" id="inp-prenume" value="First name"></label></p>
    <p><label>Parola: <input id="parl" type="password" required name="parola" value=""></label></p>
    <p id="p-reintrod"><label>Reintroduceti parola: <input id="rparl" required type="password" name="rparola"
                value=""></label></p>
    <p><label>Email: <input type="text" name="email" id="inp-email" required value="example1234@gmail.com"
                size="50"></label></p>
    <p><label>Culoare text chat: <select name="culoare_chat" id="sel-culoare">
                <option value="red" selected>Rosu</option>
                <option value="blue">Albastru</option>
                <option value="green">Verde</option>
            </select></label></p>
    <p><label>Poza: <input type="file" name="poza"></label></p>
    <p>
        <input type="submit" class="btn btn-primary btn-lg" value="Trimite">
        <input type="reset" class="btn btn-secondary btn-lg" value="Reseteaza">
    </p>
</form>